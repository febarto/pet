{
  "name": "agenda-petshop-server",
  "version": "1.0.0",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "build": "prisma generate && prisma db push && node prisma/seed.js",
    "dev": "node --watch src/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:push": "prisma db push",
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "luxon": "^3.4.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "prisma": "5.22.0"
  },
  "engines": { "node": ">=18" }
}
